{
  "name": "splitmate-api",
  "version": "1.0.0",
  "description": "Splitmate API",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "env-cmd -e dev nodemon ./src/index.js",
    "prod": "env-cmd -e prod nodemon ./src/index.js",
    "test": "jest --runInBand --detectOpenHandles",
    "test-local": "env-cmd -e test jest --runInBand --detectOpenHandles"
  },
  "nodemonConfig": {
    "ignore": [
      "snapshot/*"
    ]
  },
  "author": "Roger Floriano (petruki)",
  "dependencies": {
    "@sendgrid/mail": "^7.2.6",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.9",
    "multer": "^1.4.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "switcher-client": "^2.0.8",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rice-Beans/splitmate-api"
